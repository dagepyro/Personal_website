<!--Carousel Wrapper-->
<div id="carousel-example-2" class="carousel slide carousel-fade" data-ride="carousel">
  <!--Indicators-->
  <ol class="carousel-indicators">
    <li data-target="#me and Sonya" data-slide-to="0" class="active"></li>
    <li data-target="#Coco's" data-slide-to="1" class="active"></li>
    <li data-target="#house view" data-slide-to="2"></li>
    <li data-target="#Alonaki Beach" data-slide-to="3"></li>
    <li data-target="#cows" data-slide-to="4"></li>
    <li data-target="#family" data-slide-to="5"></li>
  </ol>
  <!--/.Indicators-->
  <!--Slides-->
  <div class="carousel-inner" role="listbox">
    <div class="carousel-item active">
      <div class="view">
        <img class="d-block w-25 mx-auto" src="/img/Me and Sonya.jpg" alt="Me and my wife">
        <div class="carousel-caption">
          <h1 class="white-text">Me and my wife</h1>
        </div>
      </div>
    </div>
    <div class="carousel-item">
      <div class="view">
        <img class="d-block w-50 mx-auto" src="/img/Cocos.jpg"
          alt="Cocos curry LA">
      </div>
      <div class="carousel-caption">
        <h3 class="h3-responsive">Cocos ichibanya</h3>
        <p>pork cuttlet curry, :)</p>
      </div>
    </div>
    <div class="carousel-item">
      <!--Mask color-->
      <div class="view">
        <img class="d-block w-50 mx-auto" src="/img/view from house.jpg"
          alt="Second slide">
      </div>
      <div class="carousel-caption">
        <h3 class="h3-responsive">Creete greece</h3>
        <p>Ardoliges vilage</p>
      </div>
    </div>
    <div class="carousel-item">
      <div class="view">
        <img class="d-block w-50 mx-auto" src="/img/Alonaki beach.jpg" alt="Alonaki Beach">
      </div>
      <div class="carousel-caption">
        <p>Watching the sun set at Aloaki Beach</p>
      </div>
    </div>
    <div class="carousel-item">
      <!--Mask color-->
      <div class="view">
        <img class="d-block w-50 mx-auto" src="/img/Cows.jpg"
          alt="Third slide">
      </div>
      <div class="carousel-caption">
        <h3 class="h3-responsive">Cows</h3>
        <p>some of my cows</p>
      </div>
    </div>
    <div class="carousel-item">
      <div class="view">
        <img class="d-block w-50 mx-auto" src="/img/Cousins.jpg" alt="children of 1987">
        <div class="carousel-caption">
          <h4 class="h4-responsive">Me and my cousins</h4>
        </div>
      </div>
    </div>
  </div>
  <!--/.Slides-->
  <!--Controls-->
  <a class="carousel-control-prev" href="#carousel-example-2" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only deep-purple-text">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carousel-example-2" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only deep-purple">Next</span>
  </a>
